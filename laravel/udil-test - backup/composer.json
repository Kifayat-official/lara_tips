{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2.5|^8.0",
        "barryvdh/laravel-dompdf": "^2.0",
        "barryvdh/laravel-snappy": "^1.0",
        "beyondcode/laravel-er-diagram-generator": "^2.0",
        "dompdf/dompdf": "^2.0",
        "erusev/parsedown": "^1.7",
        "fideloper/proxy": "^4.4",
        "guzzlehttp/guzzle": "^7.8",
        "jakub-onderka/php-console-color": "^0.2.0",
        "jakub-onderka/php-console-highlighter": "^0.4.0",
        "knplabs/knp-snappy": "^1.4",
        "laravel/framework": "^6.20.26",
        "laravel/tinker": "^2.5",
        "laravelcollective/html": "^6.4",
        "lukascivil/treewalker": "^0.41.0",
        "maatwebsite/excel": "^3.1",
        "phenx/php-font-lib": "^0.5.4",
        "phenx/php-svg-lib": "^0.5.0",
        "phpdocumentor/reflection-docblock": "^5.3",
        "phpdocumentor/type-resolver": "^1.7",
        "phpspec/prophecy": "^1.17",
        "ralouphie/getallheaders": "^3.0",
        "sabberworm/php-css-parser": "^8.4",
        "webmozart/assert": "^1.11",
        "webpatser/laravel-uuid": "^4.0",
        "yajra/laravel-datatables": "^1.5",
        "yajra/laravel-datatables-buttons": "^4.13",
        "yajra/laravel-datatables-editor": "^1.25",
        "yajra/laravel-datatables-fractal": "^1.6",
        "yajra/laravel-datatables-html": "^4.41",
        "yajra/laravel-datatables-oracle": "^9.21"
    },
    "require-dev": {
        "facade/ignition": "^1.16.15",
        "fakerphp/faker": "^1.9.1",
        "guzzlehttp/promises": "^2.0",
        "guzzlehttp/psr7": "^2.6",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "phpdocumentor/reflection-common": "^2.2",
        "phpunit/phpunit": "^8.5.8|^9.3.3"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
